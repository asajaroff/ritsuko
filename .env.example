# Ritsuko Bot Configuration
# Copy this file to .env and fill in your actual values

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=DEBUG

# Zulip Configuration
ZULIP_EMAIL="your-bot@chat.example.com"
ZULIP_API_KEY="your-zulip-api-key-here"
ZULIP_SITE="https://chat.example.com"

# GitHub Token (for Matchbox integration)
GITHUB_MATCHBOX_TOKEN="github_pat_your_token_here"

# Nautobot Configuration
NAUTOBOT_TOKEN="your-nautobot-token-here"
NAUTOBOT_URL="https://nautobot.example.com/api"

# Anthropic API Key (for Claude integration)
ANTHROPIC_API_KEY="your-anthropic-api-key-here"
